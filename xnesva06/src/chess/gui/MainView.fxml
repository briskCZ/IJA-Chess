<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="chess.gui.MainController">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="Game">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newGameClicked" text="New game"/>
                        <MenuItem mnemonicParsing="false" onAction="#loadGameClicked" text="Load game"/>
                        <MenuItem mnemonicParsing="false" onAction="#saveGameClicked" text="Save game"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TabPane fx:id="tabPane" VBox.vgrow="ALWAYS"/>
        <Label fx:id="infoLabel">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>

    </children>
</VBox>
